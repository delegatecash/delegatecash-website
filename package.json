{
  "name": "delegatecash-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "routify -c dev:vite",
    "dev:vite": "vite",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "build": "vite build",
    "prebuild": "routify -b -D",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"**/*.svelte\" \"**/*.ts\" \"**/*.js\"",
    "format-check": "prettier --check \"**/*.svelte\" \"**/*.ts\" \"**/*.js\""
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.8",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.12",
    "buffer": "^6.0.3",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.0",
    "vite-plugin-node-stdlib-browser": "^0.1.1"
  },
  "dependencies": {
    "@web3-onboard/core": "^2.10.0",
    "@web3-onboard/injected-wallets": "^2.3.0",
    "@web3-onboard/walletconnect": "^2.1.3",
    "delegatecash": "^0.3.0",
    "delegatecash-button-svelte": "^0.1.1",
    "ethers": "^5.7.1",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.29.0"
  }
}
